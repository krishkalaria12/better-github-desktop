{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "deep-link:default",
    "os:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://api.github.com/*"
        },
        {
          "url": "https://github.com/*"
        },
        {
          "url": "https://raw.githubusercontent.com/*"
        }
      ],
      "deny": []
    },
    "dialog:default",
    "store:default"
  ]
}